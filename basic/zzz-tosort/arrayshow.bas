10 ' ARRAYSHOW.BAS
20 DIM L,I,J,X,Y,XS,YS
30 XS=4:YS=5:DIM BX(XS,YS) '2 dims, 3x3
40 FOR X=0 TO XS-1
50 FOR Y=0 TO YS-1
60 BX(X,Y)=Y:PRINT Y;
70 NEXT:PRINT
80 NEXT
90 L=PEEK(29)*256+PEEK(30)
100 PRINT L;
110 PRINT CHR$(PEEK(L));CHR$(PEEK(L+1));
120 PRINT PEEK(L+2)*256+PEEK(L+3);
130 PRINT PEEK(L+4)
140 FOR I=0 TO PEEK(L+4)-1
150 PRINT PEEK(L+5+I*2)*256+PEEK(L+5+I*2+1);
160 NEXT:PRINT
170 FOR I=0 TO PEEK(L+4-1)




