10 REM piano.bas
20 CLS5
30 PRINT @256," C   D  E   F  G   A  B   C   D"
35 PRINT @0,"   C# D#     F#  G# A#     B#"
40 FOR A=0 TO 9
50 FOR B=2 TO 15
60 RESET(A*7,B)
70 NEXT:NEXT
80 FOR A=0 TO 63:RESET(A,15):NEXT
90 FOR A=0 TO 8
100 IF A=2 OR A=6 THEN NEXT
110 FOR B=2 TO 10
120 RESET(5+A*7,B)
130 RESET(6+A*7,B)
135 IF A=8 THEN 160
140 RESET(8+A*7,B)
150 RESET(9+A*7,B)
160 NEXT:NEXT
170 PRINT @256+32,;
