10 WL=10
20 DIM LL(100)
30 L=1024:LI=0:D=1:C=0
40 CLS 0
50 FOR I=1 TO 10:POKE 1023+RND(512),191:NEXT
60 POKE L,207:LL(LI)=L
70 LI=LI+1:IF LI>=WL THEN LI=0
80 EI=LI+1:IF EI>=WL THEN EI=0
90 IF LL(EI)<>0 THEN POKE LL(EI),128
100 C=C+1:IF C=10 THEN IF WL<100 THEN WL=WL+1:SOUND 100,1:C=0
110 A$=INKEY$
120 IF A$="W" THEN D=-32
130 IF A$="A" THEN D=-1
140 IF A$="D" THEN D=1
150 IF A$="S" THEN D=32
160 NL=L+D
170 IF NL>1023 THEN IF NL<1536 THEN IF PEEK(NL)=128 THEN L=NL:GOTO 60
180 SOUND 1,10:PRINT"CRASH!"
190 GOTO 190
