0 REM Checksum
5 POKE 65495,0
6 PMODE 4,1:PCLS:SCREEN 1,1
10 SZ=100:MX=255
20 DIM CK(255)
25 ' INIT COUNTERS
30 FOR I=0 TO 255:CK(I)=0:NEXT
35 ' CREATE CHECKSUM
40 CK=0
50 FOR I=0 TO SZ-1:CK=CK+RND(256)-1:NEXT
60 IF CK>MX THEN CK=CK-MX-1:GOTO 60
70 CK(CK)=CK(CK)+1
71 PSET(CK,CK(CK))
72 IF CK(CK)=255 THEN 999
75 'IF INKEY$<>"" THEN GOSUB 100
80 GOTO 40
100 FOR A=0 TO 255:PRINT CK(A);:NEXT:PRINT:RETURN
999 GOTO 999
