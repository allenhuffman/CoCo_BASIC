10 G=RND(100):LG=1:HG=100
20 PRINT:PRINT "I GUESS YOUR NUMBER IS";G
30 PRINT "TOO H)IGH, L)OW OR C)ORRECT?"
40 A$=INKEY$:IF A$="" THEN 40
50 IF A$="H" THEN HG=G:G=LG+INT((HG-LG)/2):GOTO 20
60 IF A$="L" THEN LG=G:G=LG+INT((HG-LG)/2):GOTO 20
70 IF A$="C" THEN PRINT "I KNEW IT!":END
80 SOUND 1,1:GOTO 40
