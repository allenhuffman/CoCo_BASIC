10 REM HEX LOADER FOR EXTRA COMMANDS
20 CLEAR 200,31599
30 INPUT "START";ST
40 INPUT "END";ED
50 FOR J=ST TO ED STEP 8
60 PRINT USING "##### : ";J;
70 INPUT A$
80 CS=0
90 FOR K=1 TO LEN(A$)
100 CS=CS+K*VAL("&H"+MID$(A$,K,1))
110 NEXT K
120 INPUT " = ";C
130 IF C<>CS THEN PRINT "CHECKSUM ERROR":SOUND 1,1:GOTO 60
140 FOR K=0 TO 7
160 NEXT K,J
